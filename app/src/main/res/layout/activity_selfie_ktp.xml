<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/constraintLayoutSelfi"
    android:layout_width="match_parent"
    android:layout_height="match_parent"

    tools:context="com.example.androiddatadatabinding.Activity.SelfieKtp">

    <com.wonderkiln.camerakit.CameraView
        android:id="@+id/SelfiSurfaceView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <com.example.androiddatadatabinding.GraphicUtils.GraphicOverlay
        android:id="@+id/graphic_overlay"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="0dp"
        android:layout_height="450dp"
        app:layout_constraintEnd_toEndOf="@+id/graphic_overlay"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/graphic_overlay"
        app:layout_constraintTop_toTopOf="@+id/graphic_overlay"
        app:srcCompat="@drawable/ic_ic_scaner_face_02"
        tools:ignore="VectorDrawableCompat" />

    <View
        android:id="@+id/view5"
        android:layout_width="202dp"
        android:layout_height="97dp"
        android:layout_marginStart="32dp"
        android:layout_marginEnd="32dp"
        android:background="@drawable/border"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView" />

    <ImageView
        android:id="@+id/imagecapture"
        android:layout_width="51dp"
        android:layout_height="52dp"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="16dp"
        android:paddingRight="10dp"
        android:paddingBottom="10dp"
        android:src="@drawable/ic_camera_black_24dp"
        app:layout_constraintBottom_toBottomOf="@+id/graphic_overlay"
        app:layout_constraintEnd_toEndOf="@+id/graphic_overlay" />
</android.support.constraint.ConstraintLayout>
