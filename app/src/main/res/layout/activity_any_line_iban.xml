<?xml version="1.0" encoding="utf-8"?>
<android.support.design.widget.CoordinatorLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.androiddatadatabinding.Activity.AnyLineIBAN">

    <android.support.constraint.ConstraintLayout
        android:id="@+id/constraintLayoutktp"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <io.anyline.view.ScanView
            android:id="@+id/scanview"
            android:layout_width="match_parent"
            android:layout_height="match_parent"

            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="1.0"
            tools:ignore="MissingConstraints" />

        <View
            android:id="@+id/border_anyline"
            android:layout_width="match_parent"
            android:layout_height="230dp"
            android:layout_margin="8dp"
            android:layout_marginStart="8dp"
            android:layout_marginEnd="8dp"
            android:background="@drawable/border"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.39999998" />

        <ImageView
            android:id="@+id/borderImageKtp"
            android:layout_width="90dp"
            android:layout_height="106dp"
            app:layout_constraintBottom_toBottomOf="@+id/border_anyline"
            app:layout_constraintEnd_toEndOf="@+id/border_anyline"
            app:layout_constraintHorizontal_bias="0.934"
            app:layout_constraintStart_toStartOf="@+id/border_anyline"
            app:layout_constraintTop_toTopOf="@+id/border_anyline"
            app:layout_constraintVertical_bias="0.37"
            app:srcCompat="@drawable/ic_ic_scaner_face_01"
            tools:ignore="VectorDrawableCompat" />

        <View
            android:id="@+id/view"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:background="#9F020202"
            app:layout_constraintBottom_toTopOf="@+id/border_anyline"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/scanview" />

        <View
            android:id="@+id/view4"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:background="#9F020202"
            app:layout_constraintBottom_toBottomOf="@+id/border_anyline"
            app:layout_constraintEnd_toEndOf="@+id/scanview"
            app:layout_constraintStart_toEndOf="@+id/border_anyline"
            app:layout_constraintTop_toBottomOf="@+id/view" />

        <View
            android:id="@+id/view3"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:background="#9F020202"
            app:layout_constraintBottom_toBottomOf="@+id/border_anyline"
            app:layout_constraintEnd_toStartOf="@+id/border_anyline"
            app:layout_constraintStart_toStartOf="@+id/scanview"
            app:layout_constraintTop_toBottomOf="@+id/view" />

        <View
            android:id="@+id/view2"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:background="#9F020202"
            app:layout_constraintBottom_toBottomOf="@+id/scanview"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/border_anyline" />
        <TextView
            android:id="@+id/textView4"
            android:layout_width="216dp"
            android:layout_height="42dp"
            android:layout_marginStart="32dp"
            android:layout_marginTop="8dp"
            android:layout_marginEnd="32dp"
            android:text="@string/text_recod_ktp"
            android:textColor="@color/textcolor"
            app:layout_constraintBottom_toTopOf="@+id/border_anyline"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/scanview" />
    </android.support.constraint.ConstraintLayout>

</android.support.design.widget.CoordinatorLayout>
